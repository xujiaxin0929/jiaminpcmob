  .public-nav-136_3 .innerBan {
    /* display: none; */
    position: relative; }

  .public-nav-136_3 .innerBan h1 {
    width: 100%;
    color: #fff;
    font-size: .7rem;
    position: absolute;
    text-align: center;
    top: 50%;
    transform: translateY(-50%);
    -webkit-transform: translateY(-50%); } }

