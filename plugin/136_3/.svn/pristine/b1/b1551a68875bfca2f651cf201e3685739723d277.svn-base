.public-nav-136_3 {
  width: 100%; }

.public-nav-136_3 .banner {
  width: 100%;
  height: 480px;
  position: relative; }

.public-nav-136_3 .bn_box {
  width: 100%;
  height: 64px;
  position: absolute;
  left: 0;
  bottom: 0;
  background: rgba(0, 0, 0, 0.6); }

.public-nav-136_3 .bn_nav {
  text-align: center; }

.public-nav-136_3 .bn_nav li {
  display: inline-block;
  line-height: 64px;
  width: auto;
  border-left: 1px solid #9fa7a6; }

.public-nav-136_3 .bn_nav li a:hover {
  background: #0e6eb8; }

.public-nav-136_3 .bn_nav li a {
  display: block;
  width: 168px;
  font-size: 16px;
  color: #fff;
  -webkit-transition: all 0s ease-in-out;
  -moz-transition: all 0s ease-in-out;
  -ms-transition: all 0s ease-in-out;
  -o-transition: all 0s ease-in-out;
  transition: all 0s ease-in-out; }

.public-nav-136_3 .bn_nav li:last-child {
  border-right: 1px solid #9fa7a6; }

.public-nav-136_3 .bn_nav .active a {
  color: #fff;
  background: #0e6eb8; }

@media screen and (min-width: 992px) and (max-width: 1200px) {
  .public-nav-136_3 .bn_nav li a {
    width: 140px; } }
@media screen and (min-width: 768px) and (max-width: 992px) {
  .public-nav-136_3 .bn_nav li {
    width: 14.28571%; }

  .public-nav-136_3 .bn_nav li a {
    width: 100%;
    font-size: 14px; } }
@media screen and (min-width: 769px) {
  .hide_p {
    display: none !important; } }
@media screen and (max-width: 768px) {
  .hide_m {
    display: none !important; }

  .public-nav-136_3 .banner {
    height: 3.6rem;
    position: relative; }

  .public-nav-136_3 .bn_box {
    height: 0.8rem;
    background: #ECECEC;
    overflow: hidden; }

  .public-nav-136_3 .bn_nav {
    overflow: auto;
    white-space: nowrap; }

  .public-nav-136_3 .bn_nav li {
    width: auto;
    overflow: auto;
    height: 0.8rem;
    border: none !important;
    margin: 0 0.22rem; }

  .public-nav-136_3 .bn_nav li a {
    box-sizing: border-box;
    width: auto;
    font-size: 0.26rem;
    color: #5e5e5e;
    line-height: 0.78rem; }

  .public-nav-136_3 .bn_nav .active a {
    color: #5e5e5e;
    background: none;
    border-bottom: 0.02rem solid #0e6eb8; }

  .public-nav-136_3 .navs_scroll {
    position: absolute;
    height: 0.9rem;
    width: 100%;
    box-sizing: border-box;
    overflow: hidden;
    bottom: 0;
    left: 0; }

  .public-nav-136_3 .subNavsList {
    width: 100%;
    height: .9rem;
    background: #ebebeb;
    box-sizing: border-box;
    padding: 0 .15rem;
    overflow: hidden; }

  .public-nav-136_3 .subNavsList.fixed {
    position: fixed;
    top: 0;
    left: 0;
    background: #f60;
    z-index: 99; }

  .public-nav-136_3 .subNavsList.fixed li a {
    color: #fff; }

  .public-nav-136_3 .orange .subNavsList {
    background: #f60; }

  .public-nav-136_3 .orange .subNavsList li a {
    color: #fff; }

  .public-nav-136_3 .navs_scroll ul {
    white-space: nowrap; }

  .public-nav-136_3 .navs_scroll li {
    display: inline-block;
    width: auto !important;
    overflow: hidden;
    padding: 0 .2rem; }

  .public-nav-136_3 .navs_scroll li.active a {
    border-bottom-color: #0e6eb8; }

  .public-nav-136_3 .navs_scroll li a {
    display: block;
    text-align: center;
    line-height: 0.88rem;
    color: #000;
    font-size: 0.32rem;
    box-sizing: border-box;
    border-bottom: 0.02rem solid transparent;
    -webkit-transition: all 0s ease-in-out;
    -moz-transition: all 0s ease-in-out;
    -ms-transition: all 0s ease-in-out;
    -o-transition: all 0s ease-in-out;
    transition: all 0s ease-in-out; }

  .public-nav-136_3 .navs_scroll li.cur a {
